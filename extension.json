{
	"name": "Simple Paste Image",
	"version": "0.0.2",
	"author": [
		"Urfiner (Nikolai Kochkin)"
	],
	"url": "",
	"type": "media",
	"description": "To simplify pasting documents with images in VisualEditor",
	"descriptionmsg": "simplepasteimage-desc",
	"requires": {
		"MediaWiki": ">= 1.39.2",
		"extensions": {}
	},
	"Hooks": {
		"VisualEditorBeforeInitialize": "MediaWiki\\Extension\\MyImageUploader\\MyImageUploaderHooks::onVisualEditorBeforeInitialize"
	},
	"MessagesDirs": {
		"SimplePasteImage": [
			"i18n"
		]
	},
	"config": {
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "SimplePasteImage/modules"
	},
	"ResourceModules": {
		"ext.simplePasteImage.imageHandler": {
			"localBasePath": "modules/imageHandler",
			"remoteExtPath": "SimplePasteImage/modules/imageHandler",
			"packageFiles": [
				"imageHandler.js"
			],
			"targets": [
				"desktop"
			]
		},
		"ext.simplePasteImage.imagePasteProcessor": {
			"localBasePath": "modules/imagePasteProcessor",
			"remoteExtPath": "SimplePasteImage/modules/imagePasteProcessor",
			"packageFiles": [
				"imagePasteProcessor.js"
			],
			"dependencies": [
				"ext.simplePasteImage.imageHandler",
				"ext.visualEditor.core",
				"ext.visualEditor.desktopArticleTarget.init",
				"ext.visualEditor.targetLoader",
				"mediawiki.api"
			],
			"targets": [
				"desktop"
			]
		}
	},
	"attributes": {
		"VisualEditor": {
			"PluginModules": [
				"ext.simplePasteImage.imagePasteProcessor"
			]
		}
	},
	"manifest_version": 2
}